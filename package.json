{
  "name": "visual-tool",
  "productName": "Visual Spanning Tree",
  "version": "1.0.0",
  "description": "Visual Spanning Tree is an algorithm visualization app developed by Linda Zhou",
  "main": "index.js",
  "author": "Linda Zhou",
  "license": "",
  "build": {
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "build/static_assets/mac/icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "build/static_assets/win/icon.png"
    },
    "files": [
      "node_modules/",
      "build/",
      "static_assets/",
      "*"
    ]
  },
  "scripts": {
    "webpack:build": "webpack --config webpack.js",
    "webpack:watch": "webpack --config webpack.js --watch",
    "electron:dev": "electron .",
