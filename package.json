    "d3": "^5.12.0",
    "d3-selection": "^1.4.0",
    "electron-is-dev": "0.3.0",
    "nedb": "1.8.0",
    "react": "15.4.2",
    "react-d3-graph": "^2.3.0",
    "react-dom": "15.4.2",
    "react-redux": "5.0.7",
