{
  "name": "visual-tool",
  "productName": "Visual Minimum Spanning Tree",
  "version": "1.13.0",
  "description": "Visual Minimum Spanning Tree is an algorithm visualization app developed by Linda Zhou",
  "main": "index.js",
  "author": "Linda Zhou <k1763829@kcl.ac.uk>",
  "license": "",
  "build": {
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "build/static_assets/mac/icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "build/static_assets/win/icon.png"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "icon": "build/static_assets/linux/icon.png"
    },
    "extraResources": [
      "data/**"
    ],
    "files": [
      "node_modules/",
      "build/",
      "static_assets/",
      "*"
    ]
  },
  "scripts": {
    "webpack:build": "webpack --config webpack.js",
    "webpack:watch": "webpack --config webpack.js --watch",
    "electron:dev": "NODE_ENV=dev electron .",
    "pack": "electron-builder --dir",
    "dist": "NODE_ENV=prod electron-builder",
    "test": "mocha --require @babel/register ./tests/**/*.js",
    "coverage": "nyc --reporter html --reporter text npm test",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "babel-loader": "^8.0.6",
    "classnames": "2.2.6",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "electron": "2.0.18",
    "electron-builder": "^21.2.0",
    "extract-loader": "2.0.1",
    "file-loader": "2.0.0",
    "node-sass": "^4.13.0",
    "react-confirm-alert": "2.0.5",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "toastr": "2.1.4",
    "webpack": "^4.41.2"
  },
  "dependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@babel/runtime": "^7.7.6",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.9.3",
    "coveralls": "^3.0.9",
    "d3": "^5.12.0",
    "d3-context-menu": "^1.1.2",
    "d3-selection": "^1.4.0",
    "electron-is-dev": "0.3.0",
    "hazardous": "^0.3.0",
    "html-react-parser": "^0.10.0",
    "html-to-image": "^0.1.1",
    "mocha": "^6.2.2",
    "nedb": "1.8.0",
    "nedb-promises": "^4.0.1",
    "nyc": "^14.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-chartjs-2": "^2.8.0",
    "react-d3-graph": "^2.3.0",
    "react-dom": "^16.11.0",
    "react-icons": "^3.8.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-spinners": "^0.6.1",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "tingodb": "^0.6.1",
    "webpack-cli": "^3.3.10"
  }
}
